{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongo",
    "relations": {
      "devices": {
        "type": "hasMany",
        "model": "Device",
        "foreignKey": "userId"
      }
    }
  },
  "AccessToken": {
    "dataSource": "mongo",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "User",
        "foreignKey": "userId"
      }
    }
  },
  "ACL": {
    "dataSource": "mongo",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongo",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongo",
    "public": false
  },
  "Subscription": {
    "dataSource": "mongo",
    "public": false
  },
  "Action": {
    "dataSource": "mongo",
    "public": false
  },
  "Message": {
    "dataSource": "mongo",
    "public": false
  },
  "Answer": {
    "dataSource": "mongo",
    "public": false
  },
  "Service": {
    "dataSource": "mongo",
    "public": true
  },
  "Devices": {
    "dataSource": "mongo",
    "public": true
  },
  "SubscriptionMessage": {
    "dataSource": "mongo",
    "public": false
  }
}
